<?xml version="1.0" encoding="UTF-8"?>
<zk>
	<session-config>
		<device-type>ajax</device-type>
		<timeout-message>Sesion Caducada, por favor Actualice el Navegador
		</timeout-message>
	</session-config>

	<library-property>
		<name>org.zkoss.zul.theme.fontSizeM</name>
		<value>14px</value>
	</library-property>

	<library-property>
		<name>org.zkoss.zul.theme.fontSizeS</name>
		<value>12px</value>
	</library-property>

	<library-property>
		<name>org.zkoss.zul.progressbox.position</name>
		<value>center,center</value>
	</library-property>

	<library-property>
		<name>org.zkoss.zul.progressbox.mask</name>
		<value>true</value>
	</library-property>

	<library-property>
		<name>org.zkoss.zul.progressbox.msg</name>
		<value>"Por Favor, Espere..."</value>
	</library-property>

<library-property>
		<name>org.zkoss.theme.preferred</name>
		<value>atlantic</value> or silvertail, atlantic, sapphire
	</library-property>
	<system-config>
		<ui-factory-class>org.zkoss.zk.ui.http.SerializableUiFactory
		</ui-factory-class>
	</system-config>

</zk>